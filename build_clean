KERNEL_DIR=$PWD
export KBUILD_BUILD_HOST="Sup-Mint"
export KBUILD_BUILD_USER="Yaro330"
make clean
make mrproper
export ARCH=arm
make f1xy_defconfig
make -j16 ARCH=arm CROSS_COMPILE=/home/$USER/build/mocha/arm-eabi-6.x/bin/arm-eabi- zImage
cp $KERNEL_DIR/arch/arm/boot/zImage $KERNEL_DIR/build/zImage
